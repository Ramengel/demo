{
	"name": "test",
	"version": "1.0.0",
	"description": "",
	"main": "index.js",
	"prisma": {
		"seed": "ts-node prisma/seed.ts"
	},
	"scripts": {
		"start": "node ./dist/src/index.js",
		"dev": "nodemon --config ./nodemon.json src/index.ts",
		"build": "tsc",
		"lint:fix": "eslint ./src/** --fix",
		"dev:inspect": "nodemon -e ts,json --exec node --inspect=localhost:9222 -r ts-node/register src/index.ts",
		"debug": "nodemon --exec \"node --inspect=localhost:9222 --require ts-node/register src/index.ts\"",
		"generate": "npx prisma generate",
		"migrate": "npx prisma migrate dev",
		"migrate:prod": " npx prisma migrate deploy",
		"seed": "ts-node prisma/seed.ts",
		"prod:docker": "concurrently \"npm run generate\" \"npm run migrate:prod\" \"npm run seed\" \"npm run build\" \"npm run start\"",
		"dev:docker": "concurrently \"npm run generate\" \"npm run migrate\" \"npm run seed\" \"npm run dev\"",
		"lint": "eslint ./src/**",
		"test": "jest",
		"codegen": "graphql-codegen --config codegen.ts"
	},
	"author": "",
	"license": "ISC",
	"dependencies": {
		"@prisma/client": "^4.8.0",
		"@telegram-apps/init-data-node": "^1.0.4",
