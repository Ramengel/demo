		"@ton/crypto": "^3.2.0",
		"@ton/ton": "^14.0.0",
		"adm-zip": "^0.5.10",
		"bcryptjs": "^2.4.3",
		"bn.js": "^5.2.1",
		"body-parser": "^1.20.1",
		"class-transformer": "^0.5.1",
		"class-validator": "^0.13.2",
		"concurrently": "^8.2.1",
		"cors": "^2.8.5",
		"dotenv": "^16.0.3",
		"express": "^4.18.2",
		"inversify": "^6.0.1",
		"ioredis": "^5.3.2",
		"jsonwebtoken": "^9.0.0",
		"multer": "^1.4.5-lts.1",
		"reflect-metadata": "^0.1.13",
		"slugify": "^1.6.6",
		"tslog": "^3.3.4",
		"tweetnacl": "^1.0.3",
		"telegram": "^2.26.8"
	},
